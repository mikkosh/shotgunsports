<resources>
<layout id="LapInputLayout">
	<label id="lap" text="1" x="32%" y="8%" font="Gfx.FONT_NUMBER_MILD" color="Gfx.COLOR_LT_GRAY" justification="Gfx.TEXT_JUSTIFY_RIGHT" />
	<label id="lapLabel" text="@Strings.label_turn" x="32%" y="25%" font="Gfx.FONT_XTINY" color="Gfx.COLOR_DK_GRAY" justification="Gfx.TEXT_JUSTIFY_LEFT" />
	<label id="maxLapLabel" text="/5" x="32%" y="8%" font="Gfx.FONT_NUMBER_MILD" color="Gfx.COLOR_DK_GRAY" justification="Gfx.TEXT_JUSTIFY_LEFT" />
	
	<label id="shots" text="--" x="68%" y="8%" font="Gfx.FONT_NUMBER_MILD" color="Gfx.COLOR_LT_GRAY" justification="Gfx.TEXT_JUSTIFY_CENTER" />
	<label id="shotsLabel" text="@Strings.label_turn_shots" x="68%" y="25%" font="Gfx.FONT_XTINY" color="Gfx.COLOR_DK_GRAY" justification="Gfx.TEXT_JUSTIFY_CENTER" />
	
	<label id="hitsLabel" text="@Strings.label_hits_padded" x="32%" y="65%" font="Gfx.FONT_XTINY" color="Gfx.COLOR_LT_GRAY" justification="Gfx.TEXT_JUSTIFY_CENTER" />
	<label id="missesLabel" text="@Strings.label_misses_padded" x="68%" y="65%" font="Gfx.FONT_XTINY" color="Gfx.COLOR_LT_GRAY" justification="Gfx.TEXT_JUSTIFY_CENTER" />
	
	<label id="hits" text="0" x="32%" y="center" font="Gfx.FONT_NUMBER_THAI_HOT" color="Gfx.COLOR_ORANGE" justification="Gfx.TEXT_JUSTIFY_CENTER" />
	<label id="misses" text="0" x="68%" y="center" font="Gfx.FONT_NUMBER_THAI_HOT" color="Gfx.COLOR_ORANGE" justification="Gfx.TEXT_JUSTIFY_CENTER" />
	

	
	<drawable id="finishLapSymbol" class="ArcButton">
		<param name="position">@ArcButton.POSITION_ESC_BUTTON</param>
		<param name="width">50</param>
		<param name="height">50</param>
		<param name="behavior">:onNextRoundMenu</param>
		<param name="stateDefault">@Drawables.ArcBtnCheckmark</param>
		<param name="stateHighlighted">@Drawables.ArcBtnCheckmark</param>
	</drawable>
	<drawable id="nextItemSymbol" class="ArcButton">
		<param name="position">@ArcButton.POSITION_SELECT_BUTTON</param>
		<param name="width">50</param>
		<param name="height">50</param>
		<param name="behavior">:onSelect</param>
		<param name="stateDefault">@Drawables.ArcBtnRight</param>
		<param name="stateHighlighted">@Drawables.ArcBtnRight</param>
	</drawable>
	<drawable id="increaseSymbol" class="ArcButton">
		<param name="position">@ArcButton.POSITION_UP_BUTTON</param>
		<param name="width">50</param>
		<param name="height">50</param>
		<param name="behavior">:onPreviousPage</param>
		<param name="stateDefault">@Drawables.ArcBtnUp</param>
		<param name="stateHighlighted">@Drawables.ArcBtnUp</param>
	</drawable>
	<drawable id="decreaseSymbol" class="ArcButton">
		<param name="position">@ArcButton.POSITION_DOWN_BUTTON</param>
		<param name="width">50</param>
		<param name="height">50</param>
		<param name="behavior">:onNextPage</param>
		<param name="stateDefault">@Drawables.ArcBtnDown</param>
		<param name="stateHighlighted">@Drawables.ArcBtnDown</param>
	</drawable>

</layout>



<layout id="ResultLayout">
	<bitmap id="claytarget" filename="images/claytarget.png" x="70%" y="center" />
	<label id="sportName" x="center" y="10%" font="Gfx.FONT_TINY" color="Gfx.COLOR_ORANGE" justification="Gfx.TEXT_JUSTIFY_CENTER" />
	<label id="dataTitle" x="center" y="20%" font="Gfx.FONT_TINY" color="Gfx.COLOR_LT_GRAY" justification="Gfx.TEXT_JUSTIFY_CENTER" />
	
	<label text="@Strings.label_turn_hits" id="hitsLabel" x="45%" y="35%" font="Gfx.FONT_XTINY" color="Gfx.COLOR_LT_GRAY" justification="Gfx.TEXT_JUSTIFY_RIGHT|Gfx.TEXT_JUSTIFY_VCENTER" />
	<label text="@Strings.label_turn_misses" id="missesLabel" x="45%" y="50%" font="Gfx.FONT_XTINY" color="Gfx.COLOR_LT_GRAY" justification="Gfx.TEXT_JUSTIFY_RIGHT|Gfx.TEXT_JUSTIFY_VCENTER" />
	<label text="@Strings.label_turn_shots" id="shotsLabel" x="45%" y="65%" font="Gfx.FONT_XTINY" color="Gfx.COLOR_LT_GRAY" justification="Gfx.TEXT_JUSTIFY_RIGHT|Gfx.TEXT_JUSTIFY_VCENTER" />
	<label text="@Strings.label_hits_percentage" id="hitpercentLabel" x="45%" y="80%" font="Gfx.FONT_XTINY" color="Gfx.COLOR_LT_GRAY" justification="Gfx.TEXT_JUSTIFY_RIGHT|Gfx.TEXT_JUSTIFY_VCENTER" />
	
	<label id="hits" x="50%" y="35%" font="Gfx.FONT_MEDIUM" color="Gfx.COLOR_WHITE" justification="Gfx.TEXT_JUSTIFY_LEFT|Gfx.TEXT_JUSTIFY_VCENTER" />
	<label id="misses" x="50%" y="50%" font="Gfx.FONT_MEDIUM" color="Gfx.COLOR_WHITE" justification="Gfx.TEXT_JUSTIFY_LEFT|Gfx.TEXT_JUSTIFY_VCENTER" />
	<label id="shots" x="50%" y="65%" font="Gfx.FONT_MEDIUM" color="Gfx.COLOR_WHITE" justification="Gfx.TEXT_JUSTIFY_LEFT|Gfx.TEXT_JUSTIFY_VCENTER" />
	<label id="hitpercent" x="50%" y="80%" font="Gfx.FONT_MEDIUM" color="Gfx.COLOR_WHITE" justification="Gfx.TEXT_JUSTIFY_LEFT|Gfx.TEXT_JUSTIFY_VCENTER" />
</layout>
<layout id="ResultToltalsLayout">
	<bitmap id="claytarget" filename="images/claytarget.png" x="70%" y="center" />
	<label id="sportName" x="center" y="10%" font="Gfx.FONT_TINY" color="Gfx.COLOR_ORANGE" justification="Gfx.TEXT_JUSTIFY_CENTER" />
	<label id="dataTitle" x="center" y="20%" font="Gfx.FONT_TINY" color="Gfx.COLOR_LT_GRAY" justification="Gfx.TEXT_JUSTIFY_CENTER" />
	<label id="hitpercent" x="center" y="33%" font="Gfx.FONT_NUMBER_HOT" color="Gfx.COLOR_ORANGE" justification="Gfx.TEXT_JUSTIFY_CENTER" />
	<!-- label text="@Strings.label_hits_percentage" id="hitpercentLabel" x="center" y="59%" font="Gfx.FONT_XTINY" color="Gfx.COLOR_LT_GRAY" justification="Gfx.TEXT_JUSTIFY_CENTER" / -->
	
	<label id="hits" x="25%" y="65%" font="Gfx.FONT_MEDIUM" color="Gfx.COLOR_WHITE" justification="Gfx.TEXT_JUSTIFY_CENTER" />
	<label id="misses" x="50%" y="65%" font="Gfx.FONT_MEDIUM" color="Gfx.COLOR_WHITE" justification="Gfx.TEXT_JUSTIFY_CENTER" />
	<label id="shots" x="75%" y="65%" font="Gfx.FONT_MEDIUM" color="Gfx.COLOR_WHITE" justification="Gfx.TEXT_JUSTIFY_CENTER" />
	
	
	<label text="@Strings.label_turn_hits" id="hitsLabel" x="25%" y="80%" font="Gfx.FONT_XTINY" color="Gfx.COLOR_LT_GRAY" justification="Gfx.TEXT_JUSTIFY_CENTER" />
	<label text="@Strings.label_turn_misses" id="missesLabel" x="50%" y="80%" font="Gfx.FONT_XTINY" color="Gfx.COLOR_LT_GRAY" justification="Gfx.TEXT_JUSTIFY_CENTER" />
	<label text="@Strings.label_turn_shots_short" id="shotsLabel" x="75%" y="80%" font="Gfx.FONT_XTINY" color="Gfx.COLOR_LT_GRAY" justification="Gfx.TEXT_JUSTIFY_CENTER" />
	
</layout>
<layout id="AboutLayout"> 
	<label id="about_text" text="@Strings.about_author" x="50%" y="90%" font="Gfx.FONT_XTINY" color="Gfx.COLOR_LT_GRAY" justification="Gfx.TEXT_JUSTIFY_CENTER" />
</layout>
</resources>